{
  "name": "composer-form",
  "version": "0.1.0",
  "description": "Hyperledger Composer Form, code for generating a web form from a composer modeling language type",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "private": true,
  "dependencies": {
    "body-parser": "^1.18.3",
    "composer-cli": "^0.19.11",
    "composer-common": "^0.19.9",
    "debug": "2.6.2",
    "express": "^4.16.3",
    "forms": "^1.3.0"
  },
  "scripts": {
    "pretest": "npm run lint",
    "lint": "eslint .",
    "postlint": "npm run licchk",
    "licchk": "license-check-and-add",
    "postlicchk": "npm run doc",
    "mocha": "mocha --recursive -t 10000",
    "nyc": "nyc mocha --recursive -t 10000",
    "doc": "jsdoc --pedantic --recurse -c jsdoc.json",
    "test": "nyc mocha --recursive -t 10000",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "blockchain",
    "hyperledger",
    "solutions"
  ],
  "author": "Hyperledger Composer",
  "license": "Apache-2.0",
  "devDependencies": {
    "ajv": "6.1.1",
    "babel-preset-latest": "6.24.1",
    "babelify": "7.3.0",
    "browserfs": "1.1.0",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "chai-things": "0.2.0",
    "coveralls": "^3.0.1",
    "eslint": "3.17.1",
    "jsdoc": "3.5.5",
    "license-check-and-add": "2.3.1",
    "mocha": "3.4.2",
    "mockery": "2.0.0",
    "moxios": "0.4.0",
    "nunjucks": "3.0.1",
    "nyc": "^11.1.0",
    "pegjs": "0.9.0",
    "rimraf": "2.5.4",
    "sinon": "2.3.8"
  },
  "license-check-and-add-config": {
    "folder": ".",
    "license": "LICENSE.txt",
    "exact_paths_method": "EXCLUDE",
    "exact_paths": [
      "coverage",
      "LICENSE.txt",
      "node_modules",
      ".nyc-output",
      "out",
      ".git",
      "yarn.lock",
      "yarn-error.log",
      "README.md"
    ],
    "file_type_method": "EXCLUDE",
    "file_types": [
      ".yml",
      ".yaml",
      ".zip",
      ".tgz"
    ],
    "insert_license": false,
    "license_formats": {
      "js|njk|pegjs|cto|acl|qry": {
        "prepend": "/*",
        "append": " */",
        "eachLine": {
          "prepend": " * "
        }
      },
      "npmrc|editorconfig|txt": {
        "eachLine": {
          "prepend": "# "
        }
      }
    }
  },
  "nyc": {
    "exclude": [
      "coverage/**",
      "lib/codegen/javascriptparser.js",
      "lib/codegen/parsejs.js",
      "lib/introspect/parser.js",
      "lib/acl/parser.js",
      "lib/query/parser.js",
      "out/**",
      "scripts/**",
      "systest/**",
      "test/**"
    ],
    "reporter": [
      "text-summary",
      "html",
      "cobertura",
      "lcov",
      "clover",
      "text-lcov",
      "text"
    ],
    "include": [
      "lib/**.js"
    ],
    "all": true,
    "check-coverage": true,
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100
  }
}
